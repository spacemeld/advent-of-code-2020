{
    "env": {
        "node": true,
        "es2020": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-typescript/base",
        "prettier",
        "prettier/@typescript-eslint"
    ],
    "rules": {
      "no-console": "off"
    }
}
